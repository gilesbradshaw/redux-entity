{
  "name": "redux-entity",
  "version": "0.0.0-alpha.0",
  "description": "Real time CRUD redux module",
  "main": "lib/entity.js",
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "clean:lib": "rimraf lib",
    "build:commonjs": "better-npm-run build:commonjs",
    "compile": "better-npm-run compile",
    "lint": "eslint src tests server",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "codecov": "cat coverage/*/lcov.info | codecov"
  },
  "betterScripts": {
    "build:commonjs": {
      "command": "npm run clean:lib && cross-env BABEL_ENV=commonjs babel src/modules --out-dir lib",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "compile": {
      "command": "babel-node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon --exec babel-node bin/server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "babel-node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "babel-node ./node_modules/karma/bin/karma start build/karma.conf",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gilesbradshaw/redux-entity.git"
  },
  "author": "Giles Bradshaw <giles.bradshaw@8Technology.com> (http://8-technology.co.uk/)",
  "license": "MIT",
  "dependencies": {
    "rxjs": "^5.0.0-beta.8"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-decorators": "^6.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "better-npm-run": "0.0.8",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.3.17",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "chai-rx-assert": "^1.0.3",
    "chai-string": "^1.2.0",
    "cheerio": "^0.20.0",
    "co-mocha": "^1.1.2",
    "codecov": "^1.0.1",
    "cross-env": "^1.0.8",
    "eslint": "^2.12.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-config-standard-react": "^2.2.0",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-react": "^5.1.1",
    "eslint-plugin-standard": "^1.3.1",
    "imports-loader": "^0.6.5",
    "ip": "^1.1.2",
    "isparta-loader": "^2.0.0",
    "karma": "^0.13.21",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-webpack-with-fast-source-maps": "^1.9.2",
    "mocha": "^2.2.5",
    "nodemon": "^1.8.1",
    "rimraf": "^2.5.1",
    "rxjs-to-async-iterator": "^2.1.1",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.6.0"
  }
}
